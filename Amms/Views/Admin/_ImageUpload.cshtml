@model Amms.Models.Item
@using (Html.BeginForm("UploadFiles", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="text" name="Id" id="Id" value="@Model.ItemID" readonly/>
    @Html.EditorFor(model => model.file, new { htmlAttributes = new { @class = "form-control", @type = "file" } })
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Upload" class="btn btn-primary btn-third" />
        </div>
    </div>
}
